
.btn {
	margin-top: 80px;
	height: 44px;
	line-height: 44px;
	width: calc(100% - 30px);
	margin-left: 15px;
	border-radius: 4px;
	text-align: center;
	color: #FFFFFF;
	background-color: #FF5725;
}
.page {
	width: 100%;
	height: 100%;
	background-color: #FAFAFA;
}
.section {
	height: 100px;
	width: calc(100% - 30px);
	margin-left: 15px;
	background-color: #FFFFFF;
	margin-top: 15px;
	border-radius: 8px;
}
.cell {
	width: 100%;
	position: relative;
	height: 50px;
	align-items: center;
}
.celltitle {
	height: 50px;
	line-height: 50px;
	color: rgba(50, 50, 50, 1);
	font-size: 16px;
	margin-left: 15px;
}
.voicetitle {
	position: absolute;
	right: 15px;
	color: #FF5725;
	font-size: 12px;
}
.jiantou {
	position: absolute;
	right: 15px;
	width: 20px;
	height: 20px;
}

