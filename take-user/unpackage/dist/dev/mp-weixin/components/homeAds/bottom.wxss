
.self-view {
	position: fixed;
	width: 100%;
	/* height: 100%; */
	top: 0;
	left: 0;
	background-color: rgba(0, 0, 0, 0.6);
	display: flex;
	flex-direction: column;
	z-index: 999;
}
.whiteView {
	margin-bottom: 0;
	background-color: #FFF;
	border-radius:16px 16px 0 0;
	position: relative;
	display: flex;
	flex-direction: column;
}
.topTipsLabel {
	margin-left: 15px;
	margin-top: 20px;
	font-size: 16px;
	font-weight: bold;
	color: #383838;
	width: calc(100% - 30px);
}
.mapBackView {
	margin-left: 15px;
	margin-top: 10px;
	width: calc(100% - 30px);
	min-height: 174px;
	max-height: 288px;
	border-radius: 6px;
	background-color: #FFF9F8;
}
.mapView {
	margin-left: 10px;
	margin-top: 10px;
	width: calc(100% - 20px);
	height: 88px;
}
.bottomBtn {
	margin-left: 15px;
	margin-top: 20px;
	margin-bottom: 50px;
	width: calc(100% - 30px);
	height: 50px;
	border-radius: 8px;
	line-height: 50px;
	color: #FFF;
	background-color: #FF5725;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
}


