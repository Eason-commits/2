
.white_view {
	width: 80%;
	/* height: 230px; */
	background-color: #FFF;
	border-radius: 16px;
	align-items: center;
}
.text_back_view {
	margin-left: 15px;
	width: calc(100% - 30px);
	height: 110px;
	background-color: #F5F6F7;
	border-radius: 8px;
}
.text_view {
	margin-left: 5px;
	margin-top: 5px;
	width: calc(100% - 10px);
	height: 100px;
	font-size: 14px;
	font-weight: bold;
}
.bottom_button {
	margin-left: 20%;
	width: 60%;
	height: 40px;
	border-radius: 8px;
	line-height: 40px;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	background-color: #FF5725;
	color: #FFF;
	margin-top: 15px;
}

