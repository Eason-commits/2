.cellContentView {
	width: 100%;
	background-color: #FFFFFF;
	margin-bottom: 1px;
}
.iconImage {
	margin-left: 15px;
	margin-top: 20px;
	width: 40px;
	height: 40px;
	border-radius: 20px;
	background-color: #999;
}
.nameTimeView {
	margin-left: 8px;
	margin-top: 20px;
	height: 40px;
	flex: 1;
}
.userNickeName {
	height: 20px;
	line-height: 20px;
	font-size: 15px;
	color: #343434;
}
.textContent {
	margin-left: 15px;
	width: calc(100% - 30px);
	line-height: 20px;
	font-size: 14px;
	font-weight: bold;
	color: #323232;
}
.middle-Content {
	margin-left: 15px;
	width: calc(100% - 30px);
	display: flex;
	flex-wrap: wrap;
	position: relative;
	margin-top: 10px;
}

