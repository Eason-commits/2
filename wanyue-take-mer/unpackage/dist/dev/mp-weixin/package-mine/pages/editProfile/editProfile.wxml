<view><scroll-view style="{{('height:'+windowHeight+'px')}}" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['changeLogo']]]]}}" class="list_cell" bindtap="__e"><text class="list_cell_title">{{i18n.mine.shoplogo}}</text><image class="list_cell_avatar" src="{{thumbPath}}" mode="aspectFill"></image><image class="list_cell_right1" src="../../../static/ads-seld-right.png" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['changeName']]]]}}" class="list_cell2" bindtap="__e"><text class="list_cell_title">{{i18n.mine.shopName}}</text><text class="list_cell_right_txt">{{shopName}}</text><image class="list_cell_right" src="../../../static/ads-seld-right.png" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['changePhone']]]]}}" class="list_cell2" bindtap="__e"><text class="list_cell_title">{{i18n.mine.shopphone}}</text><text class="list_cell_right_txt">{{shopPhone}}</text><image class="list_cell_right" src="../../../static/ads-seld-right.png" mode="aspectFit"></image></view><view class="list_cell2"><text class="list_cell_title">{{i18n.mine.shoptype}}</text><text class="list_cell_right_txt">{{shopType}}</text><image class="list_cell_right" src mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['changeShopAddress']]]]}}" class="list_cell2" bindtap="__e"><text class="list_cell_title">{{i18n.mine.shopaddress}}</text><text class="list_cell_right_txt">{{address}}</text><image class="list_cell_right" src="../../../static/ads-seld-right.png" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['save']]]]}}" class="bottomBtn" bindtap="__e">{{i18n.mine.save}}</view></scroll-view></view>