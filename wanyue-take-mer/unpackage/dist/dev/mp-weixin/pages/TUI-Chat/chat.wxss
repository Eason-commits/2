.container {
	width: 100vw;
	height: 100vh;
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	display: flex;
	flex-direction: column;
}
.tui-chatroom-navigatorbar {
	position: relative;
	/*top: 0;*/
	flex-shrink: 0;
	width: 750rpx;
	height: 176rpx;
	background-color: #FFF;
}
.tui-chatroom-navigatorbar-back {
	position: absolute;
	width: 48rpx;
	height: 48rpx;
	left: 24rpx;
	bottom: 20rpx;
}
.conversation-title {
	position: absolute;
	width: 80%;
	height: 88rpx;
	line-height: 56rpx;
	font-size: 36rpx;
	color: #181818;
	/* margin-left: 100px; */
	/* z-index: 100; */
	bottom: 0;
	left: 100rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.message-list {
	flex: 1;
	width: 100vw;
	overflow-y: scroll;
}
.message-input {
	flex-shrink: 0;
	width: 100%;
}
.calling {
	position: fixed;
	z-index: 199;
	top: 0;




	bottom: 0;
	right: 0;
}
.group-profile {
	top: 176rpx;
	left: 0;
	z-index: 1111
}
.container-box {
	position: fixed;
	display: flex;
	justify-content: center;
	align-items: center;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	background-color: rgba(0, 0, 0, 0.5);
}
.video-message {
	width: 90vw;
	height: 300px;
}
.msgorder{
	height: 32px; 
	width:100%;
	background-color: rgba(0, 193, 222, 0.2);
	display: flex;
	flex-direction: row;
	/* justify-content: center; */
	align-items: center;
}
.order-img{
	width: 15px;
	height: 15px;
	margin-left: 10px;
}
.orderID{
	display: flex;
	flex: 1;
	margin-left: 10px;
	margin-right: 10px;
	font-size: 14px;
	text-align: left;
	color: #00c1de;
}
.orderback{
	margin-right: 10px;
	width: 15px;
	height: 15px;
}

