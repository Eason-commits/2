
page{
	width: 100%;
	height: 100%;
	background-color: #FAFAFA;
}
.showView{
	margin-left: 12px;
	margin-top: 10px;
	width: calc(100% - 24px);
	border-radius: 8px 8px 0px 0px;
	background-color: #FFFFFF;
	overflow: hidden;
}
.circle_quan{
	margin-top: 10px;
	margin-left: 12px;
	width: 4px;
	height: 4px;
	border-radius: 4px;
	border: 2px solid #00C1DE;
}
.circle_shi{
	margin-top: 10px;
	margin-left: 13px;
	width: 6px;
	height: 6px;
	border-radius: 3px;
	background-color: #D8D8D8;
}
.xian{
	margin-top: 5px;
	margin-left: 16px;
	width: 1px;
	margin-bottom: 0;
	flex: 1;
	background-color: #D8D8D8;
}

