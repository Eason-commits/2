
page {
	width: 100%;
	height: 100%;
	overflow: hidden;
	background-color: #f5f6f7;
}
.section {
	width: 100%;
	background-color: #FFF;
	display: flex;
	flex-direction: row;
	height: 52px;
	border-bottom: 1px solid #f5f6f7;
	align-items: center;
}
.title {
	font-size: 16px;
	color: #181818;
	flex: 1;
	margin-left: 15px;
}

