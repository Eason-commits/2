
page {
	width: 100%;
	height: 100%;
	background-color: #F5F6F7;
	overflow: hidden;
}
.nameBackView {
	width: calc(100% - 30px);
	margin-left: 15px;
	height: 45px;
	line-height: 45px;
	align-items: center;
	border-bottom: 0.5px solid #D6DADE;
}
.addBtn {
	align-items: center;
	justify-content: center;
	border: 1px dashed #989898;
}
.save{
	position: absolute;
	left: 15px;
	right: 15px;
	font-size: 16px;
	bottom: 30px;
	height: 44px;
	line-height: 44px;
	text-align: center;
	align-items: center;
	justify-content: center;
	border-radius: 4px;
	background-color: #FFF;
}

